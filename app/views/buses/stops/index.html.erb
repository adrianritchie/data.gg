
<h2>Stops</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Latitude</th>
      <th>Longitude</th>
    </tr>
  </thead>
  <tbody>
    <% @stops.each do |stop| %>
    <tr>
      <td><%= stop.name %></td>
      <td><%= stop.latitude %></td>
      <td><%= stop.longitude %></td>
      <td>Edit</td>
      <td>Delete</td>
    </tr>
    <% end %>
    <%= form_for @stop %>
    <tr>
      <td></td>
      <td></td>
      <td></td>
    </tr>
    <% end %>
  </tbody>
</table>